<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:app.Pages"
             x:Class="app.Pages.FlyoutMainPage"
             Title="FlyoutMainPage"
            FlyoutLayoutBehavior="Popover">

    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="flyoutMenuPage"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:Calendar/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>